<?xml version="1.0" encoding="UTF-8"?>
<odoo>
  <record id="ia_rental_schedule_view_search_inherit_lots" model="ir.ui.view">
    <field name="name">ia.rental.schedule.search.inherit.stock</field>
    <field name="model">sale.rental.schedule</field>
    <field name="inherit_id" ref="sale_renting.rental_schedule_view_search"/>
    <field name="priority">100</field>
    <field name="arch" type="xml">
      <field name="product_id" position="after">
          <field name="home_depot_id"/>
      </field>
      <xpath expr="//filter[@name='Lot']" position="after">
        <filter string="Home Depot" name="Lot" context="{'group_by': 'home_depot_id'}"/>
      </xpath>
    </field>
  </record>
